/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Or(a=a, b=sel, out=aort);
    Not(in=sel, out=negsel);
    Or(a=b, b=negsel, out=bort);
    And(a=aort, b=bort, out=out);
}